<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="-10dp"
    tools:layout_editor_absoluteY="0dp">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="386dp"
        app:layout_constraintStart_toEndOf="@+id/restofPlaceNums"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/red_heart" />

    <TextView
        android:id="@+id/firstPlaceNum"
        android:layout_width="33dp"
        android:layout_height="18dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="216dp"
        android:text="TextView"
        android:gravity="right"
        android:textAlignment="gravity"
        app:layout_constraintStart_toStartOf="@+id/firstPlaceText"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="216dp"
        app:layout_constraintStart_toEndOf="@+id/firstPlaceNum"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/red_heart" />

    <TextView
        android:id="@+id/secondPlaceNum"
        android:layout_width="33dp"
        android:layout_height="18dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="236dp"
        android:text="TextView"
        android:gravity="right"
        android:textAlignment="gravity"
        app:layout_constraintStart_toStartOf="@+id/secondPlaceText"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="236dp"
        app:layout_constraintStart_toEndOf="@+id/secondPlaceNum"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/red_heart" />

    <Button
        android:id="@+id/btnDaily"
        android:layout_width="110dp"
        android:layout_height="54dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="12dp"
        android:text="Daily"
        app:layout_constraintEnd_toStartOf="@+id/btnWeekly"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnWeekly"
        android:layout_width="110dp"
        android:layout_height="54dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="12dp"
        android:text="Weekly"
        app:layout_constraintEnd_toStartOf="@+id/btnLifetime"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnLifetime"
        android:layout_width="110dp"
        android:layout_height="54dp"
        android:layout_marginTop="108dp"
        android:layout_marginEnd="20dp"
        android:text="Lifetime"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/thirdPlaceText"
        android:layout_width="70dp"
        android:layout_height="110dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="260dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/firstPlaceText"
        android:layout_width="70dp"
        android:layout_height="110dp"
        android:layout_marginStart="148dp"
        android:layout_marginTop="216dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/secondPlaceText"
        android:layout_width="70dp"
        android:layout_height="110dp"
        android:layout_marginStart="272dp"
        android:layout_marginTop="236dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/restOfPlacesText"
        android:layout_width="250dp"
        android:layout_height="300dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="384dp"
        android:maxLines="10"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/thirdPlaceNum"
        android:layout_width="33dp"
        android:layout_height="18dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="260dp"
        android:gravity="right"
        android:text="TextView"
        android:textAlignment="gravity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="260dp"
        app:layout_constraintStart_toEndOf="@+id/thirdPlaceNum"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/red_heart" />

    <TextView
        android:id="@+id/restofPlaceNums"
        android:layout_width="33dp"
        android:layout_height="180dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="384dp"
        android:gravity="right"
        android:text="TextView"
        android:textAlignment="gravity"
        app:layout_constraintStart_toEndOf="@+id/restOfPlacesText"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/restofPlaceNums"
        app:layout_constraintTop_toBottomOf="@+id/imageView5"
        app:srcCompat="@drawable/red_heart" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/restofPlaceNums"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:srcCompat="@drawable/red_heart" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@+id/restofPlaceNums"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:srcCompat="@drawable/red_heart" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/restofPlaceNums"
        app:layout_constraintTop_toBottomOf="@+id/imageView9"
        app:srcCompat="@drawable/red_heart" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/restofPlaceNums"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:srcCompat="@drawable/red_heart" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="14dp"
        android:layout_height="18dp"
        android:layout_marginTop="3dp"
        app:layout_constraintStart_toEndOf="@+id/restofPlaceNums"
        app:layout_constraintTop_toBottomOf="@+id/imageView11"
        app:srcCompat="@drawable/red_heart" />

    <Button
        android:id="@+id/stringRequestButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="StrReq"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>