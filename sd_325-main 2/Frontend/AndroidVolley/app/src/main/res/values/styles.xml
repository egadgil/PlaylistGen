<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="moss" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimaryDark">@color/mossDark</item>
        <item name="colorPrimary">@color/mossDark</item>
        <item name ="android:colorBackground">@color/moss</item>
        <item name="colorAccent">@color/mossCom</item>
        <item name="colorButtonNormal">@color/mossButton</item>

    </style>
    <style name="skyblye" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/skyblueDark</item>
        <item name="colorPrimaryDark">@color/skyblueDark</item>
        <item name ="android:colorBackground">@color/skyblue</item>
        <item name="colorAccent">@color/skyblueCom</item>
        <item name="colorButtonNormal">@color/skyblueButton</item>

    </style>

    <style name="lilac" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/lilacDark</item>
        <item name="colorPrimaryDark">@color/lilacDark</item>
        <item name ="android:colorBackground">@color/lilac</item>
        <item name="colorAccent">@color/lilacCom</item>
        <item name="colorButtonNormal">@color/lilacButton</item>
    </style>

    <style name="plain" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

    </style>

</resources>
